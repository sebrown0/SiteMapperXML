<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
  targetNamespace="ElementType" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  elementFormDefault="qualified" 
  xmlns:dt="TestData">
  
  <xs:import schemaLocation="http://www.w3.org/2001/03/xml.xsd"
    namespace="http://www.w3.org/XML/1998/namespace"/>

  <xs:import 
    schemaLocation="file:/C:/SiteMapperXML/Schema/data/TestData.xsd" 
    namespace="TestData"/>

  <xs:element name="TestDataIn">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Data" type="dt:Data"/>
      </xs:sequence>

      <xs:attribute ref="xml:base"/>
    </xs:complexType>
  </xs:element>
</xs:schema>


